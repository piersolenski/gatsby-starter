{
  "name": "gatsby-starter",
  "private": true,
  "description": "A simple starter project to get up and developing quickly with Gatsby",
  "version": "2.0.0",
  "author": "Piers Olenski",
  "dependencies": {
    "@reach/router": "^1.3.4",
    "gatsby": "^2.24.69",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sitemap": "^2.4.15",
    "gatsby-starter-bootstrap": "git+https://github.com/superfunkminister/gatsby-starter-bootstrap.git",
    "polished": "^3.6.7",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "WTFPL",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run dev",
    "bootstrap": "node node_modules/gatsby-starter-bootstrap",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/**/*.js"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.3.17",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-react-helmet": "^3.3.13",
    "gatsby-plugin-sharp": "^2.6.39",
    "gatsby-plugin-styled-components": "^3.3.13",
    "gatsby-source-filesystem": "^2.3.33",
    "gatsby-transformer-sharp": "^2.5.17",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superfunkminister/gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/superfunkminister/gatsby-starter/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.+(json|css|md)": "prettier --write"
  }
}
